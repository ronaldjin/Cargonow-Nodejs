div
    .m
        sv-link-back
    .fs-headline.ml.mr Radio Program OCA
    md-divider.m
    div(layout='row' class='mb ml mr')
        .mr
            sv-fb-like
        .mr
            sv-fb-share
        .mr
            sv-google-share
    .fs-subhead.ml.mr.mb.tc-grey-600 {{dt}}
    div(ng-if='program')
        .ml.mr
            iframe(ng-src='{{convertUrl(program.youtube)}}' frameborder="0" allowfullscreen)
        div
            .fs-headline.ml.mr Program Sponsors:
            ul
                li.card.p.m(ng-repeat='guest in program.guests')
                    div {{guest.name}}
                    div
                        a(href='{{guest.webUrl}}' target='_blank') {{guest.webAnkor}}
                    div {{guest.phone}}
                    div
                        a(href='{{guest.webUrl}}' target='_blank') {{guest.webUrl.replace('http://','')}}

