@import 'nib'

.sv-dialog-md {
  min-width 70%
  min-height auto
  @media screen and (max-width: 992px) {
    min-width 90%
    min-height auto
  }
}

.cl-height
  height 200px

[sv-animate-on-change]
  position relative
  top 0px
  opacity 1
  transition: all 1s;
  -webkit-transition: all 1s;

[sv-animate-on-change].changed
  top -200px
  opacity 0
  position relative
  transition: all 1s;
  -webkit-transition: all 1s;



